<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Basic.SignUp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SignUp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04" />

    <EditText
        android:id="@+id/sign_up_First_Name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:drawableStart="@drawable/icons_namelast"
        android:hint="Name"
        app:layout_constraintBottom_toTopOf="@+id/sign_up_Last_Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.936" />

    <EditText
        android:id="@+id/sign_up_Last_Name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:hint="Sirname"
        android:drawableStart="@drawable/icons_name"
        app:layout_constraintBottom_toTopOf="@+id/sign_up_email_id"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/sign_up_email_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:hint=" Email Id"
        android:drawableStart="@drawable/icons_email"
        app:layout_constraintBottom_toTopOf="@+id/sign_up_Phone_No"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/sign_up_Phone_No"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:hint=" Phone No."
        android:drawableStart="@drawable/icons_phonecall"
        app:layout_constraintBottom_toTopOf="@+id/Sign_Up_password"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/Sign_Up_password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint=" Password"
        android:drawableStart="@drawable/icons_passwords"
        app:layout_constraintBottom_toTopOf="@+id/sign_up_renter_password"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/sign_up_renter_password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="136dp"
        android:drawableStart="@drawable/icons_passwords"
        android:hint="Confirm Password"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/logojiit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.099"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/Sign_Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign_In"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.931" />


</androidx.constraintlayout.widget.ConstraintLayout>

</layout>